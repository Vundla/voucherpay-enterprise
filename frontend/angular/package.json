{
  "name": "voucherpay-enterprise-frontend",
  "version": "1.0.0",
  "description": "Angular frontend for VoucherPay Enterprise - Inclusive Platform",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --host 0.0.0.0 --port 4200",
    "build": "ng build",
    "build:prod": "ng build --configuration production",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "test:coverage": "ng test --code-coverage",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "analyze": "ng build --configuration production --stats-json && npx webpack-bundle-analyzer dist/stats.json",
    "accessibility:audit": "pa11y-ci --sitemap http://localhost:4200/sitemap.xml",
    "accessibility:test": "axe --dir dist --save axe-report.json"
  },
  "dependencies": {
    "@angular/animations": "^17.0.0",
    "@angular/cdk": "^17.0.0",
    "@angular/common": "^17.0.0",
    "@angular/compiler": "^17.0.0",
    "@angular/core": "^17.0.0",
    "@angular/forms": "^17.0.0",
    "@angular/material": "^17.0.0",
    "@angular/platform-browser": "^17.0.0",
    "@angular/platform-browser-dynamic": "^17.0.0",
    "@angular/router": "^17.0.0",
    "@angular/service-worker": "^17.0.0",
    "@ngrx/store": "^17.0.0",
    "@ngrx/effects": "^17.0.0",
    "@ngrx/store-devtools": "^17.0.0",
    "apollo-angular": "^5.0.0",
    "@apollo/client": "^3.8.0",
    "graphql": "^16.8.0",
    "rxjs": "~7.8.0",
    "socket.io-client": "^4.7.0",
    "chart.js": "^4.4.0",
    "ng2-charts": "^5.0.0",
    "ngx-spinner": "^17.0.0",
    "ngx-toastr": "^17.0.0",
    "file-saver": "^2.0.5",
    "tslib": "^2.6.0",
    "zone.js": "~0.14.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.0.0",
    "@angular/cli": "^17.0.0",
    "@angular/compiler-cli": "^17.0.0",
    "@types/jasmine": "~5.1.0",
    "@types/file-saver": "^2.0.7",
    "jasmine-core": "~5.1.0",
    "karma": "~6.4.0",
    "karma-chrome-headless": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "typescript": "~5.2.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "@angular-eslint/builder": "^17.0.0",
    "@angular-eslint/eslint-plugin": "^17.0.0",
    "@angular-eslint/eslint-plugin-template": "^17.0.0",
    "@angular-eslint/schematics": "^17.0.0",
    "@angular-eslint/template-parser": "^17.0.0",
    "pa11y-ci": "^3.0.1",
    "@axe-core/cli": "^4.8.0",
    "webpack-bundle-analyzer": "^4.10.0"
  },
  "keywords": [
    "angular",
    "accessibility",
    "empowerment",
    "inclusive",
    "disabilities",
    "wcag",
    "a11y"
  ],
  "author": "VoucherPay Enterprise Team",
  "license": "MIT"
}
